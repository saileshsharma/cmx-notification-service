    <changeSet id="014-add-login-credentials" author="system">
        <!-- Add username and password columns to surveyor table -->
        <addColumn tableName="surveyor">
            <column name="username" type="VARCHAR(64)"/>
        </addColumn>
        <addColumn tableName="surveyor">
            <column name="password" type="VARCHAR(128)"/>
        </addColumn>

        <!-- Add unique constraint on username -->
        <addUniqueConstraint tableName="surveyor" columnNames="username" constraintName="uk_surveyor_username"/>
    </changeSet>

